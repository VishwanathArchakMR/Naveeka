{
  "name": "naveeka-backend",
  "version": "2.0.0",
  "description": "Backend API for Naveeka app - AI-powered travel knowledge graph",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "lint": "echo \"Add ESLint config or replace this with: eslint --ext .js ./\"",
    "seed": "node scripts/seedData.js",
    "seed:admin": "node scripts/seed_admin.js",
    "seed:regions": "node scripts/seed_regions.js",
    "seed:regions:fresh": "node scripts/seed_regions.js --clear",
    "seed:activities": "node scripts/seed_activities.js",
    "seed:airports": "node scripts/seed_airports.js",
    "seed:bus-stops": "node scripts/seed_bus_stops.js",
    "seed:buses": "node scripts/seed_buses.js",
    "seed:flights": "node scripts/seed_flights.js",
    "seed:hotels": "node scripts/seed_hotels.js",
    "seed:locations": "node scripts/seed_locations_master.js",
    "seed:messages": "node scripts/seed_messages.js",
    "seed:planning": "node scripts/seed_planning.js",
    "seed:restaurants": "node scripts/seed_restaurants.js",
    "seed:trails": "node scripts/seed_trails.js",
    "seed:train-stations": "node scripts/seed_train_stations.js",
    "seed:trains": "node scripts/seed_trains.js",
    "seed:reset:activities": "node scripts/seed_activities.js --reset",
    "seed:reset:airports": "node scripts/seed_airports.js --reset",
    "seed:reset:bus-stops": "node scripts/seed_bus_stops.js --reset",
    "seed:reset:buses": "node scripts/seed_buses.js --reset",
    "seed:reset:flights": "node scripts/seed_flights.js --reset",
    "seed:reset:hotels": "node scripts/seed_hotels.js --reset",
    "seed:reset:locations": "node scripts/seed_locations_master.js --reset",
    "seed:reset:messages": "node scripts/seed_messages.js --reset",
    "seed:reset:planning": "node scripts/seed_planning.js --reset",
    "seed:reset:restaurants": "node scripts/seed_restaurants.js --reset",
    "seed:reset:trails": "node scripts/seed_trails.js --reset",
    "seed:reset:train-stations": "node scripts/seed_train_stations.js --reset",
    "seed:reset:trains": "node scripts/seed_trains.js --reset",
    "seed:demo": "npm-run-all -s seed:regions seed:locations seed:airports seed:activities seed:restaurants seed:hotels seed:trails seed:bus-stops seed:buses seed:train-stations seed:trains seed:flights"
